{
    "functions": {
        "api/index.js": {
            "includeFiles": "backend/config/sql-wasm.wasm"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api|assets|favicon|index.html|.*\\.).*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "frontend/dist"
}
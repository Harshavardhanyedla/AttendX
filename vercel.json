{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)\\.(.*)",
            "destination": "/frontend/dist/$1.$2"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/index.html"
        }
    ],
    "outputDirectory": "frontend/dist"
}